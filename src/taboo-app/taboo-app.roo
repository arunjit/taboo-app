:host {
  display: block;
  font-family: "RobotoDraft", sans-serif;
  position: relative;
  width: 100%;
  height: 100%;
}

paper-fab {
  bottom: 20px;
  position: absolute;
  right: 20px;
}

[drawer] {
  background-color: #fff;
}
[drawer] core-toolbar {
  background-color: #4F7DC9;
  color: #fff;
}

[main] {
  background-color: #f1f1f3;
}
[main] core-toolbar {
  background-color: #526E9C;
  color: #fff;
}
